{
    "name": "iagofelicio/geo-maps",
    "autoload": {
        "psr-4": {
            "Iagofelicio\\GeoMaps\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iagofelicio\\GeoMaps\\Tests\\": "tests"
        }
    },
    "require": {
        "statamic/cms": "^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.0 || ^9.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Geo Maps",
            "description": "Geo Maps addon"
        },
        "laravel": {
            "providers": [
                "Iagofelicio\\GeoMaps\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
